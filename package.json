{
  "name": "lsh-framework",
  "version": "1.1.0",
  "description": "Encrypted secrets manager with automatic rotation, team sync, and multi-environment support. Built on a powerful shell with daemon scheduling and CI/CD integration.",
  "main": "dist/app.js",
  "bin": {
    "lsh": "./dist/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gwicho38/lsh.git"
  },
  "homepage": "https://github.com/gwicho38/lsh#readme",
  "bugs": {
    "url": "https://github.com/gwicho38/lsh/issues"
  },
  "scripts": {
    "build": "tsc",
    "compile-ts": "tsc -b",
    "compile-nexe": "node build.js",
    "build-bin": "npm run clean; npm run compile-ts; npm run compile-nexe",
    "start": "node dist/app.js",
    "start:pipeline": "npm run compile-ts && node dist/pipeline/pipeline-service.js",
    "watch": "tsc --watch",
    "test": "node --experimental-vm-modules ./node_modules/.bin/jest",
    "test:coverage": "node --experimental-vm-modules ./node_modules/.bin/jest --coverage",
    "test:integration": "npm run test -- --testMatch=\"**/*.integration.test.(js|ts)\"",
    "clean": "rm -rf ./build; rm -rf ./bin; rm -rf ./dist",
    "electron": "npm run compile-ts && electron src/electron/main.cjs",
    "electron-dev": "NODE_ENV=development npm run electron",
    "dashboard": "npm run electron",
    "app": "npm run electron",
    "lint": "eslint src --ext .js,.ts,.tsx",
    "lint:fix": "eslint src --ext .js,.ts,.tsx --fix",
    "typecheck": "tsc --noEmit",
    "audit:security": "npm audit --audit-level moderate"
  },
  "keywords": [
    "secrets-manager",
    "secrets",
    "env-manager",
    "dotenv",
    "environment-variables",
    "encryption",
    "credential-management",
    "team-sync",
    "secrets-rotation",
    "multi-environment",
    "devops",
    "security",
    "shell",
    "automation",
    "cron",
    "daemon",
    "job-scheduler",
    "cicd",
    "cli"
  ],
  "engines": {
    "node": ">=20.18.0",
    "npm": ">=10.0.0"
  },
  "type": "module",
  "author": {
    "name": "gwicho38",
    "email": "luis@lefv.io"
  },
  "license": "MIT",
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE",
    ".env.example",
    "package.json"
  ],
  "dependencies": {
    "@deck.gl/core": "^8.9.33",
    "@inkjs/ui": "^1.0.0",
    "@octokit/rest": "^20.1.0",
    "@supabase/supabase-js": "^2.57.4",
    "@types/ink": "^2.0.3",
    "@types/ioredis": "^4.28.10",
    "@types/socket.io": "^3.0.1",
    "@xstate/react": "^4.1.0",
    "async": "^3.2.5",
    "async-lock": "^1.4.0",
    "axios": "^1.5.1",
    "bcrypt": "^5.1.1",
    "chalk": "^5.3.0",
    "cheerio": "^1.0.0-rc.12",
    "chokidar": "^3.6.0",
    "commander": "^10.0.1",
    "cors": "^2.8.5",
    "csv": "^6.3.5",
    "dotenv": "^16.4.5",
    "express": "^5.1.0",
    "fprint": "^2.0.1",
    "glob": "^10.3.12",
    "gradstop": "^2.2.3",
    "helmet": "^8.1.0",
    "highlightjs": "^9.16.2",
    "http-proxy-middleware": "^3.0.5",
    "ink": "^4.4.1",
    "ink-text-input": "^5.0.1",
    "inquirer": "^9.2.12",
    "ioredis": "^5.8.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "lsh-framework": "^0.8.2",
    "node-cron": "^3.0.3",
    "node-fetch": "^3.3.2",
    "ora": "^8.0.1",
    "path": "^0.12.7",
    "pg": "^8.16.3",
    "react": "^18.2.0",
    "repl": "^0.1.3",
    "sendgrid": "^5.2.3",
    "socket.io": "^4.8.1",
    "uuid": "^10.0.0",
    "xstate": "^5.9.1",
    "zx": "^7.2.3"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.23.2",
    "@babel/preset-react": "^7.24.1",
    "@types/async-lock": "^1.4.2",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.12.7",
    "@types/react": "^18.2.73",
    "@types/request": "^2.48.12",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.44.1",
    "babel-jest": "^29.7.0",
    "electron": "^38.1.2",
    "eslint": "^9.36.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "jest": "^29.7.0",
    "mocha": "^10.3.0",
    "ncc": "^0.3.6",
    "nexe": "^4.0.0-rc.2",
    "nodemon": "^3.0.1",
    "supertest": "^7.1.4",
    "ts-jest": "^29.2.5",
    "typescript": "^5.4.5",
    "zapier-platform-core": "15.4.1"
  }
}
