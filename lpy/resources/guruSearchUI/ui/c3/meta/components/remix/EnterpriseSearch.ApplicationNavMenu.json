{
  "type": "UiSdlConnected<UiSdlNavMenu>",
  "component": {
    "logoImageUrl": "assets/images/generativeAiLogo.svg",
    "itemSections": [
      {
        "items": [
          {
            "id": "Dashboard",
            "icon": "SEARCH",
            "title": "EnterpriseSearch.ApplicationNavMenu.Search.title",
            "redirectRoute": {
              "urlPath": "/search"
            }
          },
          {
            "id": "Settings",
            "icon": "SLIDERS",
            "title": "EnterpriseSearch.ApplicationNavMenu.Settings.title",
            "redirectRoute": {
              "urlPath": "/admin"
            }
          }
        ]
      }
    ],
    "footer": {
      "items": [
        // disabling theme toggle in favor of a static background
        // {
        //   "id": "EnterpriseSearch.ApplicationNavMenu.ToggleTheme",
        //   "icon": "eclipse",
        //   "title": "EnterpriseSearch.ApplicationNavMenu.ToggleTheme.title",
        //   "tooltip": "EnterpriseSearch.ApplicationNavMenu.ToggleTheme.tooltip"
        // },
        {
          "id": "EnterpriseSearch.ApplicationNavMenu.Help",
          "icon": "question-circle",
          "title": "EnterpriseSearch.ApplicationNavMenu.Help.title"
        }
      ]
    }
  },
  "effectTriggers": [
    {
      "trigger": "EnterpriseSearch.ApplicationNavMenu.MENU_ITEM_CLICK",
      "payloadStrategy": "MERGE",
      "effectType": "UiSdlEpicHandleCustomNavMenuItemClick"
    },
    {
      "trigger": "EnterpriseSearch.ApplicationNavMenu.INITIAL_RENDER",
      "payloadStrategy": "MERGE",
      "effectType": "UiSdlEpicSetInitialTheme"
    }
  ]
}
